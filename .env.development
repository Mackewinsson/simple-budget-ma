# Development Environment
# This file contains development environment variables
# Safe to commit to version control

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://dev-api.presusimple.com
EXPO_PUBLIC_ENV=development

# Google OAuth (Development)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your-dev-google-client-id
EXPO_PUBLIC_GOOGLE_CLIENT_SECRET=your-dev-google-client-secret
EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID=your-dev-expo-google-client-id
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your-dev-ios-google-client-id
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your-dev-android-google-client-id
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-dev-web-google-client-id

# Feature Flags
EXPO_PUBLIC_FEATURE_FLAGS_URL=https://dev-api.presusimple.com/api/feature-flags

# Analytics (Development)
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_ANALYTICS_DEBUG=true

# Debug Settings
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=info
