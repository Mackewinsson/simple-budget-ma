{
  "expo": {
    "name": "PresuSimple",
    "slug": "presusimple",
    "projectId": "e4ba31e1-acfd-4050-bf55-a26bdd9f5c86",
    "owner": "mackewinsson",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "presusimple",
    "privacy": "public",
    "description": "A simple and intuitive budgeting app to help you manage your finances.",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.presusimple.app",
      "supportsTablet": true,
      "buildNumber": "1"
    },
    "android": {
      "package": "com.presusimple.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-dev-client"
    ],
    "extra": {
      "API_BASE_URL": "http://localhost:3000",
      "GOOGLE_CLIENT_ID": "",
      "GOOGLE_CLIENT_SECRET": "",
      "GOOGLE_EXPO_CLIENT_ID": "",
      "GOOGLE_IOS_CLIENT_ID": "",
      "GOOGLE_ANDROID_CLIENT_ID": "",
      "GOOGLE_WEB_CLIENT_ID": "",
      "REVENUECAT_API_KEY_IOS": "",
      "EXPO_PROJECT_NAME_FOR_PROXY": "@mackewinsson/presusimple",
      "eas": {
        "projectId": "77abe935-0d1a-42ea-bae4-470f978f8d3b"
      }
    }
  }
}
