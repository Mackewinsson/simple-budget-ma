# Staging Environment
# This file contains staging environment variables
# Safe to commit to version control

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://staging-api.presusimple.com
EXPO_PUBLIC_ENV=staging

# Google OAuth (Staging)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your-staging-google-client-id
EXPO_PUBLIC_GOOGLE_CLIENT_SECRET=your-staging-google-client-secret
EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID=your-staging-expo-google-client-id
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your-staging-ios-google-client-id
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your-staging-android-google-client-id
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your-staging-web-google-client-id

# Feature Flags
EXPO_PUBLIC_FEATURE_FLAGS_URL=https://staging-api.presusimple.com/api/feature-flags

# Analytics (Staging)
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_ANALYTICS_DEBUG=false

# Debug Settings
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_LOG_LEVEL=warn
