{
  "name": "presusimple",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "start:dev": "EXPO_PUBLIC_ENV=development expo start --dev-client",
    "start:staging": "EXPO_PUBLIC_ENV=staging expo start --dev-client",
    "start:prod": "EXPO_PUBLIC_ENV=production expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:ios-local": "eas build --profile development --platform ios --local",
    "build:staging:ios": "eas build --profile staging --platform ios",
    "build:staging:android": "eas build --profile staging --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "build:local": "npx expo run:ios",
    "submit:staging:ios": "eas submit --profile staging --platform ios",
    "submit:staging:android": "eas submit --profile staging --platform android",
    "submit:prod:ios": "eas submit --profile production --platform ios",
    "submit:prod:android": "eas submit --profile production --platform android",
    "submit:ios": "eas submit --platform ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-picker/picker": "^2.11.2",
    "@tanstack/query-persist-client-core": "^5.89.0",
    "@tanstack/react-query": "^5.89.0",
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "expo": "~54.0.9",
    "expo-auth-session": "^7.0.8",
    "expo-constants": "~18.0.9",
    "expo-crypto": "^15.0.7",
    "expo-dev-client": "~6.0.15",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "^6.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "^15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^3.3.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "reactotron-react-native": "^5.1.17",
    "reactotron-redux": "^3.2.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
